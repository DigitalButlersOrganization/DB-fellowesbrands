{
  "name": "db-fellowesbrands",
  "version": "1.0.0",
  "repository": "git@github.com:DigitalButlersOrganization/DB-fellowesbrands.git",
  "author": "Victor Kasap <kasap.victor@gmail.com>",
  "license": "MIT",
  "scripts": {
    "serve": "browser-sync start --server --files '*.html, ./**/*'",
    "pretty": "npx prettier --write \"./{css,js}/**/*.{css,js}\" \"*.html\"",
    "autoprefixer": "postcss -u autoprefixer -r ./**/*.css",
    "format": "npm run pretty && npm run autoprefixer"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.5"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11"
  ],
  "dependencies": {
    "mdb-ui-kit": "^6.2.0",
    "normalize.css": "^8.0.1"
  }
}
